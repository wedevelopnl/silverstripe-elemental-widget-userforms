{
    "name": "wedevelopnl/silverstripe-elemental-widget-userforms",
    "description": "Re-usable user form widgets for silverstripe-elemental",
    "keywords": [
        "userforms",
        "widget",
        "silverstripe",
        "elemental"
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/wedevelopnl/silverstripe-elemental-widget"
        }
    ],
    "type": "silverstripe-vendormodule",
    "require": {
        "php": ">=8.1",
        "silverstripe/framework": "^4.11 || ^5.0",
        "silverstripe/cms": "^4.11 || ^5.0",
        "silverstripe/userforms": "^5.14 || ^6.0",
        "unclecheese/display-logic": "^2.0 || ^3.0",
        "gorriecoe/silverstripe-linkfield": "^1.0",
        "symbiote/silverstripe-multivaluefield": "^5.3 || ^6.0",
        "wedevelopnl/silverstripe-elemental-widget": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "WeDevelop\\ElementalWidget\\UserForm\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/installers": false,
            "silverstripe/vendor-plugin": false
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.4"
    }
}
